{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        },
        {
            "src": "*.php",
            "use": "vercel-php@0.7.2"
        },
        {
            "src": "assets/**",
            "use": "@vercel/static"
        },
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ]
}
